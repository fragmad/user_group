    "def russian_multiplication(a, b):\n",
    "    total = 0\n",
    "    while a > 1:\n",
    "        if a % 2 == 1:\n",
    "            total = total + b\n",
    "        a = a // 2\n",
    "        b = b * 2\n",
    "    return total + b"
   ]
